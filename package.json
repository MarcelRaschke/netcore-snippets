{
  "name": "netcore-snippets",
  "displayName": "Essential ASP.NET Core 6 Snippets",
  "description": "High quality Code Snippets that boost your ASP.NET Core development productivity.",
  "version": "6.0.0",
  "publisher": "doggy8088",
  "icon": "icon.png",
  "repository": "https://github.com/doggy8088/netcore-snippets",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Snippets",
    "Extension Packs"
  ],
  "activationEvents": [
    "workspaceContains:appsettings.json",
    "onLanguage:json",
    "onLanguage:csharp",
    "onLanguage:html",
    "onLanguage:razor",
    "onLanguage:aspnetcorerazor",
    "onLanguage:editorconfig",
    "onLanguage:javascript",
    "onLanguage:msbuild",
    "onLanguage:proto3"
  ],
  "main": "src/extension",
  "license": "MIT",
  "keywords": [
    ".net-core",
    "asp.net-core",
    "code-snippets"
  ],
  "extensionPack": [
    "tintoy.msbuild-project-tools",
    "EditorConfig.EditorConfig",
    "zxh404.vscode-proto3"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "vsce:publish": "vsce publish",
    "vsce:package": "vsce package",
    "vsce:publish:patch": "vsce publish patch",
    "vsce:publish:minor": "vsce publish minor",
    "vsce:publish:major": "vsce publish major"
  },
  "devDependencies": {
    "@types/vscode": "^1.39.0",
    "@types/node": "^10.16.3",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3",
    "vsce": "^1.66.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": "csharp",
        "path": "./snippets/csharp.json"
      },
      {
        "language": "proto3",
        "path": "./snippets/proto3.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "msbuild",
        "path": "./snippets/msbuild.json"
      },
      {
        "language": "editorconfig",
        "path": "./snippets/editorconfig.json"
      }
    ]
  }
}